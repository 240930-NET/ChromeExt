{
    "manifest_version": 3,
    "name": "Revature Downloads",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "Revature.png"
    },
    "content_scripts": [
    {
        "js": ["content.js"],
        "run_at": "document_end",
        "matches": ["https://app.revature.com/curriculum/detailed/*"]
    }
  ]
 }